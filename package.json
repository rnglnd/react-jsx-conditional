{
  "name": "react-jsx-conditional",
  "version": "1.0.2",
  "description": "A lightweight conditional component to use with React 16.",
  "repository": "github:rnglnd/react-jsx-conditional",
  "main": "lib/index.js",
  "author": "Steven Ringland <steven.ringland@gmail.com>",
  "license": "MIT",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/conditional.min.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "build": "babel src -d lib && BABEL_ENV=es babel src -d es",
    "precommit": "npm test && lint-staged",
    "prepublish": "rm -rf lib && rm -rf es && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "keywords": [
    "react",
    "component",
    "conditional",
    "if",
    "statement"
  ],
  "dependencies": {
    "prop-types": "15.6.2"
  },
  "peerDependencies": {
    "react": "16.x.x",
    "react-dom": "16.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.11",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.0",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.12.3",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "^3.1.14"
  }
}
